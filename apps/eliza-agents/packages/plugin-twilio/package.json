{
    "name": "@elizaos-plugins/plugin-twilio",
    "version": "1.0.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "prebuild": "pnpm clean",
        "build": "tsup --format esm",
        "dev": "tsc -w"
    },
    "author": "boolkeys",
    "license": "ISC",
    "description": "A Twilio plugin for ElizaOS that enables SMS and voice call capabilities.",
    "dependencies": {
        "@elizaos/core": "0.1.9",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "twilio": "^4.20.1",
        "uuid": "^9.0.1",
        "elevenlabs-node": "^2.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.0.0",
        "@types/uuid": "^9.0.7",
        "esbuild": "^0.19.11",
        "glob": "^10.3.10",
        "rimraf": "^5.0.5",
        "tsx": "^4.7.0",
        "typescript": "^5.0.0",
        "@types/twilio": "^3.19.3",
        "vitest": "^1.1.3",
        "@vitest/coverage-v8": "^1.0.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:plugin:1.0.0",
        "pluginParameters": {
            "TWILIO_ACCOUNT_SID": {
                "type": "string",
                "description": "Twilio Account SID"
            },
            "TWILIO_AUTH_TOKEN": {
                "type": "string",
                "description": "Twilio Auth Token"
            },
            "TWILIO_PHONE_NUMBER": {
                "type": "string",
                "description": "Twilio Phone Number in E.164 format"
            },
            "TWILIO_CHARACTER": {
                "type": "string",
                "description": "Path to the character configuration file (e.g. character.json)",
                "default": "character.json"
            },
            "WEBHOOK_BASE_URL": {
                "type": "string",
                "description": "Base URL for webhooks (e.g. ngrok URL)"
            },
            "ELEVENLABS_XI_API_KEY": {
                "type": "string",
                "description": "ElevenLabs API key for voice synthesis",
                "optional": true
            }
        }
    }
}